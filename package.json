{
  "name": "mechhub-api",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/api-service.js",
    "build": "tsc",
    "dev": "ts-node-dev src/api-service.ts",
    "generate-lock": "npm install --package-lock-only",
    "predocker-build": "npm run generate-lock",
    "docker-build": "docker build -t mechhub-api .",
    "prod-install": "npm ci --omit=dev",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "amqplib": "^0.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.0",
    "redis": "^4.6.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.3",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}

